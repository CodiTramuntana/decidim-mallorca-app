<div class="card">
  <div class="card-divider">
    <h2 class="card-title">
      <%= t('.title') %>
    </h2>
  </div>
  <div class="card-section">
  <%= form_for @organization, url: consell_mallorca_admin_authorization_config_path, html: { class: 'form' } do |f| %>
    <%= f.text_field :pinbal_user %>
    <%= f.text_field :pinbal_pwd %>
    <%= f.submit t('.submit') %>
  <% end %>
  </div>
</div>